{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.EntityFrameworkCore": "Information",
        "Microsoft.AspNetCore": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} {Properties:j}{NewLine}{Exception}",
          "restrictedToMinimumLevel": "Debug"
        }
      }
    ],
    "Properties": {
      "Environment": "Development"
    },
    "Seq": {
      "ServerUrl": "http://localhost:5341",
      "ApiKey": null
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=ModularMonolith_Dev;Username=postgres;Password=123456"
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "http://localhost:5173",
      "https://localhost:3000",
      "https://localhost:5173",
      "http://localhost:4200",
      "https://localhost:4200"
    ],
    "AllowedMethods": [
      "GET",
      "POST",
      "PUT",
      "DELETE",
      "OPTIONS",
      "PATCH"
    ],
    "AllowedHeaders": [
      "Content-Type",
      "Authorization",
      "Accept",
      "X-Requested-With",
      "Accept-Language"
    ],
    "AllowCredentials": true,
    "MaxAge": 86400
  },
  "Hsts": {
    "MaxAgeDays": 0,
    "IncludeSubdomains": false,
    "Preload": false
  },
  "RateLimit": {
    "GlobalPermitLimit": 1000,
    "GlobalWindowMinutes": 1,
    "AuthPermitLimit": 50,
    "AuthWindowMinutes": 15,
    "ApiPermitLimit": 600,
    "ApiWindowMinutes": 1
  }
}